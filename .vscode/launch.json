{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach Main Process",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "skipFiles": [
          "<node_internals>/**"
      ]
    },
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "args" : ["--inspect-brk=5858", "dist/apps/dashboard-desktop/main.js"],
      "outputCapture": "std",
      "preLaunchTask": "build:main"
    }
  ]
}
